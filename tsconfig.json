{
  "compilerOptions": {
 	"target": "ES6",
 	"module": "CommonJS",
  "lib": ["es2016", "dom"],
 	"rootDir": "./src",
 	"outDir": "./dist",
 	"esModuleInterop": true,
 	"forceConsistentCasingInFileNames": true,
 	"strict": true,
 	"noImplicitAny": true,
 	"strictFunctionTypes": true,
 	"noUnusedLocals": true,
 	"noUnusedParameters": true,
 	"skipLibCheck": true,
  "baseUrl": "./",
  "paths": {
  "*": ["node_modules/*", "src/types/*"]
  }
},
"include": ["./src/**/*.ts"],
}
