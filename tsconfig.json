{
  "compilerOptions": {
 	"target": "ES6",
 	"module": "ES6",
 	"rootDir": "./src",
 	"outDir": "./dist",
 	"esModuleInterop": true,
 	"forceConsistentCasingInFileNames": true,
 	"strict": true,
 	"noImplicitAny": true,
 	"strictFunctionTypes": true,
 	"noUnusedLocals": true,
 	"noUnusedParameters": true,
 	"skipLibCheck": true,
},
"include": ["./src/**/*.ts"],
}
